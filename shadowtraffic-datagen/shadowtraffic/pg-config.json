{
  "generators": [
    {
      "table": "pg_reactions",

      
      "fork": {
        "key": { "_gen": "uuid" },
        "stagger": { "ms": 250 }
      },

      "varsOnce": {
        "emailAddress": {
          "_gen": "string",
          "expr": "#{Internet.emailAddress}"
        }
      },

      "row": {
        "event_id": { "_gen": "var", "var": "forkKey" },

        "generated_at": {
          "_gen": "now"
        },

        "reaction": {
          "_gen": "oneOf",
          "choices": [
            "ğŸ‘",
            "â¤ï¸",
            "ğŸ‰",
            "ğŸ™Œ",
            "ğŸ‘",
            "ğŸ”¥",
            "ğŸŒˆ",
