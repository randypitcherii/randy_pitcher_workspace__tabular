            "ğŸ†",
            "âš¡ï¸",
            "ğŸŒº",
            "ğŸ’",
            "ğŸ€",
            "ğŸ«"
          ]
        },

        "user_email": { "_gen": "var", "var": "emailAddress" },

        "user_ip": {
          "_gen": "string",
          "expr": "#{Internet.ipV4Address}"
        }
      },

      "stateMachine": {
        "_gen": "stateMachine",
        "initial": "insert",
        "transitions": {
          "insert": {
            "_gen": "weightedOneOf",
            "choices": [
              { "weight": 4, "value": "update" },
              { "weight": 1, "value": "delete" }
            ]
          },
          "update": {
            "_gen": "weightedOneOf",
            "choices": [
              { "weight": 3, "value": "update" },
              { "weight": 1, "value": "delete" }
            ]
          }
        },

        "states": {
            "insert": {
                "op": "insert"
            },
            "update": {
                "op": "update",
                "where": {
                    "event_id": { "_gen": "var", "var": "forkKey" }
                }
            },
            "delete": {
                "op": "delete",
                "where": {
                    "event_id": { "_gen": "var", "var": "forkKey" }
                }
            }
        }
      },

      "localConfigs": {
        "throttle": {
          "ms": 1000
        }
      }
    }
  ],

  "connections": {
    "pg": {
      "kind": "postgres",
      "connectionConfigs": {
        "host": "postgres",
        "port": 5432,
        "username" : "postgres",
        "password" : "postgres",
        "db": "tabular"
      }
    }
  }
}