        },

        "user_id": {
          "_gen": "oneOf",
          "choices": [
            {
                "_gen": "string",
                "expr": "#{Internet.uuid}"
            },
            {
                "_gen": "lookup",
                "topic": "reactions",
                "path": [
                  "value", "user_id"
                ]
            }
          ]
        }
      }
    }
  ],
  "connections": {
