{
  "generators": [
    {
      "topic": "reactions",

      "key": "kafka_connect.reactions",
      "value": {
        "generated_at": {
          "_gen": "now"
        },

        "reaction_at": {
          "_gen": "formatDateTime",
          "ms": {
              "_gen": "uniformDistribution",
              "bounds": [
                  {
                      "_gen": "subtract",
                      "args": [
                          { "_gen": "var", "var": "now" },
                          86400000
                      ]
                  },
                  {
                      "_gen": "add",
                      "args": [
                          { "_gen": "var", "var": "now" },
                          86400000
                      ]
                  }
              ]
          },

        "reaction": {
          "_gen": "oneOf",
          "choices": [
            "👍",
            "❤️",
            "🎉",
            "🙌",
            "👏",
            "🔥",
            "🌈",
            "🎶",
            "🥳",
            "👑",
            "💯",
            "💪",
            "🙏",
            "🚀",
            "🌟",
            "💕",
            "💥",
            "🎊",
            "✨",
            "🔔",
            "🍾",
            "🎁",
            "🌹",
            "🎈",
            "🎯",
            "🏆",
            "⚡️",
            "🌺",
            "💎",
            "🎀",
            "🍫"
          ]
        },

        "user_id": {
          "_gen": "oneOf",
          "choices": [
            {
                "_gen": "string",
                "expr": "#{Internet.uuid}"
            },
            {
                "_gen": "lookup",
                "topic": "reactions",
                "path": [
                  "value", "user_id"
                ]
            }
          ]
        }
      }
    }
  ],
  "connections": {
    "kafka": {
      "kind": "kafka",
      "producerConfigs": {
        "bootstrap.servers": "kafka:9092",
        "key.serializer": "io.shadowtraffic.kafka.serdes.JsonSerializer",
        "value.serializer": "io.shadowtraffic.kafka.serdes.JsonSerializer"
      }
    }
  }
}